{
  "name": "rhinoafrica/unit-conversion-objects",
  "description": "Unit Conversion Objects Library - A Rhino Africa Initiative",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "illuminate/support": "^10.43",
    "illuminate/http": "^10.43",
    "phpunit/phpunit": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "RhinoAfrica\\UnitConversionObjects\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      {
        "RhinoAfrica\\UnitConversionObjects\\": "src"
      }
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "RhinoAfrica\\UnitConversionObjects\\Providers\\UnitConversionObjectsServiceProvider"
      ]
    }
  }
}
